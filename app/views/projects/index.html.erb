<h1>All Projects</h1>

<table>
  <tr>
    <th>company</th>
    <th>title</th>
    <th>description</th>
  </tr>
 
<% @projects.each do |project| %>
  <tr>
    <td>
        <% if project.company %>
            <%= project.company.name %>
        <% end %>
    </td>
    <td><%= project.title %></td>
    <td><%= project.description %></td>
  </tr>
<% end %>
</table>